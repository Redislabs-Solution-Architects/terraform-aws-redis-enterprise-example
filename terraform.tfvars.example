re_license = ""

#### how many data nodes, 3 minimum, (odd numbers required)
#### example: data-node-count = 3
data-node-count = 3

##### redis enterprise software instance type
##### (if redis on flash (rof) is enabled it is suggested to use i3.2xlarge)
##### example: re_instance_type = "t2.xlarge"
re_instance_type = "t2.xlarge"

##### EBS volume creation yes or no, true false
create_ebs_volumes_re = true


##### EBS volume for persistent and ephemeral storage
#### example: re-volume-size = 150
re-volume-size = "50"

##### SECURITY
##### example: allow-public-ssh = 1
allow-public-ssh = 1

##### permits unrestricted access within the specified VPC CIDR block.
##### example: open-nets = ["10.0.0.0/16"]
open-nets = ["10.0.0.0/16"]

############# Create RE Cluster Variables

#### Cluster Inputs
#### RE Cluster Username
#### example: re_cluster_username = "admin@admin.com"
re_cluster_username = "admin@admin.com"

#### RE Cluster Password
#### example: re_cluster_password = "admin"
re_cluster_password = "admin"

############# Create RE Database Variables

#### Create databases
#### Enable if you wish to create databases.  If this is enabled you must make sure there is a valid re_databases_json_file
re_databases_create = false

#### Database configurations to create. 
#### Array of database objects (https://redis.io/docs/latest/operate/rs/references/rest-api/objects/bdb/) to create using the RE REST API. Required if re_create_databases is set. (warning: no check if database already exists). The example re_databases.json.example can be saved to the default for reference.
#### Copy re_databases.json.example to re_databases.json to get started.
re_databases_json_file = "./re_databases.json"

